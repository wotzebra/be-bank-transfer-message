{
    "name": "wotz/be-bank-transfer-message",
    "description": "Generate and validate Belgian bank transfer structured messages",
    "keywords": [
        "wotz",
        "be-bank-transfer-message",
        "finance",
        "belgium",
        "bank",
        "invoice",
        "commerce",
        "order",
        "payment"
    ],
    "homepage": "https://github.com/wotzebra/be-bank-transfer-message",
    "license": "MIT",
    "require": {
        "php" : "^8.0"
    },
    "require-dev": {
        "pestphp/pest" : "^3.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\BeBankTransferMessage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\BeBankTransferMessage\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
